{
  "name": "platform-joki-backend",
  "version": "1.0.0",
  "description": "Platform Joki - Indonesia Freelance Marketplace API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "dev:staging": "cross-env NODE_ENV=staging nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "start:staging": "cross-env NODE_ENV=staging node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:staging": "dotenv -e .env.staging -o -- prisma migrate deploy",
    "prisma:migrate:prod": "dotenv -e .env.production -o -- prisma migrate deploy",
    "prisma:push": "prisma db push",
    "prisma:push:staging": "dotenv -e .env.staging -o -- prisma db push",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^6.3.0",
    "@supabase/supabase-js": "^2.95.2",
    "@types/compression": "^1.8.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "midtrans-client": "^1.4.3",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "zod": "^3.24.1"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.12.0",
    "@types/node-cron": "^3.0.11",
    "cross-env": "^10.1.0",
    "dotenv-cli": "^11.0.0",
    "nodemon": "^3.1.9",
    "prisma": "^6.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
